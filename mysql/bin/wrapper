#!/bin/sh
DYLD_FALLBACK_LIBRARY_PATH=/Applications/moodle/mysql/lib:$DYLD_FALLBACK_LIBRARY_PATH
export DYLD_FALLBACK_LIBRARY_PATH
case "$@" in
  *--no-defaults*)
    exec $0.bin "$@"
    ;;
  *--defaults-extra-file*)
    exec $0.bin "$@"
    ;;
esac
exec $0.bin --defaults-file=/Applications/moodle/mysql/my.cnf "$@"
